{
  "name": "@signalsync/database",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "migrate": "node scripts/migrate.js",
    "migrate:create": "node scripts/create-migration.js",
    "generate-types": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/types.ts",
    "reset": "node scripts/reset-db.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0"
  },
  "devDependencies": {
    "@signalsync/config-typescript": "workspace:*",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  }
}
